# APARAVI MCP Server Configuration
# Copy this file to .env and fill in your actual values

# APARAVI API Configuration
APARAVI_HOST=localhost
APARAVI_PORT=80
APARAVI_USERNAME=your_username_here
APARAVI_PASSWORD=your_password_here
APARAVI_API_VERSION=v3
APARAVI_TIMEOUT=30
APARAVI_MAX_RETRIES=3

# MCP Server Configuration
MCP_SERVER_NAME=aparavi-mcp-server
MCP_SERVER_VERSION=0.1.0
LOG_LEVEL=INFO
CACHE_ENABLED=true
CACHE_TTL=300
